 data() {\r\n    return {\r\n      cube: null,\r\n      scene: null,\r\n      camera: null,\r\n      renderer: null,\r\n      isLargeScreen: true,\r\n      hideBottomNav: false,\r\n      scrollTimer: null,\r\n      prevScrollY: 0,\r\n      showLoginModal: false,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.initThreeJS();\r\n    window.addEventListener('resize', this.handleScreenResize);\r\n    this.handleScreenResize();\r\n    window.addEventListener('scroll', this.handleScroll);\r\n  },\r\n  beforeUnmount() {\r\n    window.removeEventListener('resize', this.handleScreenResize);\r\n    window.removeEventListener('scroll', this.handleScroll);\r\n  },\r\n  methods: {\r\n    handleScreenResize() {\r\n      this.isLargeScreen = window.innerWidth >= 768 || window.scrollY === 0;\r\n      this.hideBottomNav = window.innerWidth >= 768 || window.scrollY === 0;\r\n    },\r\n    handleScroll() {\r\n      clearTimeout(this.scrollTimer);\r\n      this.cube.position.y = window.pageYOffset / 100;\r\n      if (window.pageYOffset > this.prevScrollY || window.innerWidth >= 768) {\r\n        // Scrolling down\r\n        this.hideBottomNav = true;\r\n      } else {\r\n        // Scrolling up\r\n        this.hideBottomNav = false;\r\n      }\r\n      if (window.scrollY === 0) {\r\n        // At top of page\r\n        this.hideBottomNav = true;\r\n      }\r\n      this.prevScrollY = window.pageYOffset;\r\n      this.scrollTimer = setTimeout(() => {\r\n        this.hideBottomNav = true;\r\n      }, 5000);\r\n    },\r\n    openLoginModal() {\r\n      this.showLoginModal = true;\r\n    },\r\n    closeLoginModal() {\r\n      this.showLoginModal = false;\r\n    },\r\n    initThreeJS() {\r\n      // create a scene\r\n      this.scene = new THREE.Scene();\r\n\r\n      // create a camera\r\n      this.camera = new THREE.PerspectiveCamera(\r\n        75,\r\n        window.innerWidth / window.innerHeight,\r\n        0.1,\r\n        1000\r\n      );\r\n      this.camera.position.z = 5;\r\n\r\n      // create a renderer and attach it to the DOM\r\n      this.renderer = new THREE.WebGLRenderer();\r\n      this.renderer.setSize(window.innerWidth, window.innerHeight);\r\n      document.getElementById('three-container').appendChild(this.renderer.domElement);\r\n\r\n      // create a cube\r\n      let geometry = new THREE.BoxGeometry();\r\n      let material = new THREE.MeshBasicMaterial({ color: 0x00ff00 });\r\n      this.cube = new THREE.Mesh(geometry, material);\r\n      this.scene.add(this.cube);\r\n\r\n      this.animate();\r\n    },\r\n    animate() {\r\n      requestAnimationFrame(this.animate);\r\n      this.cube.rotation.x += 0.01;\r\n      this.cube.rotation.y += 0.01;\r\n      this.renderer.render(this.scene, this.camera);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n#app {\r\n  font-family: 'Avenir', Helvetica, Arial, sans-serif;\r\n  -webkit-font-smoothing: antialiased;\r\n  -moz-osx-font-smoothing: grayscale;\r\n  text-align: center;\r\n  color: #2c3e50;\r\n  margin-top: 60px;\r\n}\r\n</style>\r\n"],"mappings":"AAQA,OAAO,KAAKA,KAAI,MAAO,OAAO;AAC9B,OAAOC,SAAQ,MAAO,4BAA4B;AAElD,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,IAAI;MACVC,KAAK,EAAE,IAAI;MACXC,MAAM,EAAE,IAAI;MACZC,QAAQ,EAAE,IAAI;MACdC,aAAa,EAAE,IAAI;MACnBC,aAAa,EAAE,KAAK;MACpBC,WAAW,EAAE,IAAI;MACjBC,WAAW,EAAE,CAAC;MACdC,cAAc,EAAE;IAClB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,WAAW,CAAC,CAAC;IAClBC,MAAM,CAACC,gBAAgB,CAAC,QAAQ,EAAE,IAAI,CAACC,kBAAkB,CAAC;IAC1D,IAAI,CAACA,kBAAkB,CAAC,CAAC;IACzBF,MAAM,CAACC,gBAAgB,CAAC,QAAQ,EAAE,IAAI,CAACE,YAAY,CAAC;EACtD,CAAC;EACDC,aAAaA,CAAA,EAAG;IACdJ,MAAM,CAACK,mBAAmB,CAAC,QAAQ,EAAE,IAAI,CAACH,kBAAkB,CAAC;IAC7DF,MAAM,CAACK,mBAAmB,CAAC,QAAQ,EAAE,IAAI,CAACF,YAAY,CAAC;EACzD,CAAC;EACDG,OAAO,EAAE;IACPJ,kBAAkBA,CAAA,EAAG;MACnB,IAAI,CAACT,aAAY,GAAIO,MAAM,CAACO,UAAS,IAAK,GAAE,IAAKP,MAAM,CAACQ,OAAM,KAAM,CAAC;MACrE,IAAI,CAACd,aAAY,GAAIM,MAAM,CAACO,UAAS,IAAK,GAAE,IAAKP,MAAM,CAACQ,