    if (this.password !== this.confirmPassword) {\r\n        alert('Passwords do not match');\r\n        return;\r\n      }\r\n\r\n      try {\r\n        const response = await axios.post('http://localhost:8000/api/users/', {\r\n          username: this.username,\r\n          password: this.password\r\n        });\r\n        console.log(response.data);\r\n\r\n        // After successful registration, log in the user\r\n        const loginResponse = await axios.post('http://localhost:8000/api/api-token-auth/', {\r\n          username: this.username,\r\n          password: this.password\r\n        });\r\n\r\n        localStorage.setItem('token', loginResponse.data.token);\r\n        this.$router.push('/protected'); // redirect to a protected page\r\n      } catch (error) {\r\n        console.error(error);\r\n      }\r\n    },\r\n    cancel() {\r\n      this.$emit('close');\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n@import url('https://fonts.googleapis.com/css2?family=Kaushan+Script&display=swap');\r\n.custom-sky{\r\n  background: #307c8a;\r\n}\r\n</style>\r\n"],"mappings":";AAmCA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,eAAe,EAAE;IACnB;EACF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,QAAQA,CAAA,EAAG;MACf,IAAI,IAAI,CAACH,QAAO,KAAM,IAAI,CAACC,eAAe,EAAE;QAC1CG,KAAK,CAAC,wBAAwB,CAAC;QAC/B;MACF;MAEA,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMR,KAAK,CAACS,IAAI,CAAC,kCAAkC,EAAE;UACpEP,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC,CAAC;QACFO,OAAO,CAACC,GAAG,CAACH,QAAQ,CAACP,IAAI,CAAC;;QAE1B;QACA,MAAMW,aAAY,GAAI,MAAMZ,KAAK,CAACS,IAAI,CAAC,2CAA2C,EAAE;UAClFP,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC,CAAC;QAEFU,YAAY,CAACC,OAAO,CAAC,OAAO,EAAEF,aAAa,CAACX,IAAI,CAACc,KAAK,CAAC;QACvD,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,YAAY,CAAC,EAAE;MACnC,EAAE,OAAOC,KAAK,EAAE;QACdR,OAAO,CAACQ,KAAK,CAACA,KAAK,CAAC;MACtB;IACF,CAAC;IACDC,MAAMA,CAAA,EAAG;MACP,IAAI,CAACC,KAAK,CAAC,OAAO,CAAC;IACrB;EACF;AA