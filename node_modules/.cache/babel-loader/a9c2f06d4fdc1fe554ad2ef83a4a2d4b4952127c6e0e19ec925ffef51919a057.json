{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      username: '',\n      password: '',\n      confirmPassword: ''\n    };\n  },\n  methods: {\n    async register() {\n      if (this.password !== this.confirmPassword) {\n        alert('Passwords do not match');\n        return;\n      }\n      try {\n        const response = await axios.post('http://localhost:8000/api/users/', {\n          username: this.username,\n          password: this.password\n        });\n        console.log(response.data);\n\n        // After successful registration, log in the user\n        const loginResponse = await axios.post('http://localhost:8000/api/api-token-auth/', {\n          username: this.username,\n          password: this.password\n        });\n        localStorage.setItem('token', loginResponse.data.token);\n        this.$router.push('/protected'); // redirect to a protected page\n      } catch (error) {\n        console.error(error);\n      }\n    },\n    cancel() {\n      this.$emit('close');\n    }\n  }\n};","map":{"version":3,"names":["axios","data","username","password","confirmPassword","methods","register","alert","response","post","console","log","loginResponse","localStorage","setItem","token","$router","push","error","cancel","$emit"],"sources":["C:\\Users\\burge\\OneDrive\\Desktop\\mh_py\\frontend\\src\\components\\RegisterView.vue"],"sourcesContent":["<template>\r\n  <div class=\"h-96 p-5 flex items-center justify-center custom-sky\">\r\n    <div class=\"bg-white p-10 rounded shadow-3xl w-full\">\r\n      <h2 class=\"text-center text-3xl font-extrabold text-gray-900 mb-7\" style=\"font-family: 'Kaushan Script', cursive;\">Register</h2>\r\n      <form @submit.prevent=\"register\">\r\n        <div class=\"space-y-6\">\r\n          <div>\r\n            <label for=\"username\" class=\"sr-only\">Username</label>\r\n            <input id=\"username\" type=\"text\" v-model=\"userna